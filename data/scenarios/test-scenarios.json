{
    "$schema": "../docs/scenario-schema.json",
    "test-simple-retirement": {
      "metadata": {
        "title": "Test: Simple Retirement",
        "description": "A basic test scenario for validation - single asset, no income",
        "tags": ["test", "simple", "retirement"],
        "difficulty": "beginner",
        "author": "System Test"
      },
      "plan": {
        "monthly_expenses": 4000,
        "duration_months": 120
      },
      "assets": [
        {
          "name": "Test Savings",
          "type": "taxable",
          "balance": 200000,
          "interest_rate": 0.04
        }
      ]
    },

    "test-with-income": {
      "metadata": {
        "title": "Test: Retirement with Income",
        "description": "Test scenario with delayed income - validates income timing",
        "tags": ["test", "income", "social-security"],
        "difficulty": "beginner",
        "author": "System Test"
      },
      "plan": {
        "monthly_expenses": 5000,
        "duration_months": 180
      },
      "income": [
        {
          "name": "Test Social Security",
          "amount": 2000,
          "start_month": 12
        }
      ],
      "assets": [
        {
          "name": "Test Portfolio",
          "type": "taxable",
          "balance": 300000,
          "interest_rate": 0.06
        }
      ]
    },

    "test-multiple-assets": {
      "metadata": {
        "title": "Test: Multiple Asset Strategy",
        "description": "Test scenario with multiple assets and withdrawal order - validates complex scenarios",
        "tags": ["test", "multiple-assets", "strategy"],
        "difficulty": "intermediate",
        "author": "System Test"
      },
      "plan": {
        "monthly_expenses": 6000,
        "duration_months": 240,
        "inflation_rate": 0.025
      },
      "income": [
        {
          "name": "Test Pension",
          "amount": 1500,
          "start_month": 60
        }
      ],
      "assets": [
        {
          "name": "Test Emergency Fund",
          "type": "taxable",
          "balance": 50000,
          "interest_rate": 0.02
        },
        {
          "name": "Test Investment Account",
          "type": "taxable",
          "balance": 400000,
          "interest_rate": 0.07
        },
        {
          "name": "Test IRA",
          "type": "tax_deferred",
          "balance": 250000,
          "interest_rate": 0.065
        }
      ],
      "order": [
        {"account": "Test Emergency Fund", "order": 1},
        {"account": "Test Investment Account", "order": 2},
        {"account": "Test IRA", "order": 3}
      ]
    },

    "test-new-rate-system": {
      "metadata": {
        "title": "Test: New Rate Schedule System",
        "description": "Test scenario using the new rate schedule system instead of legacy interest_rate",
        "tags": ["test", "rate-schedules", "advanced"],
        "difficulty": "advanced",
        "author": "System Test"
      },
      "plan": {
        "monthly_expenses": 7000,
        "duration_months": 300,
        "inflation_schedule": "test_inflation"
      },
      "assets": [
        {
          "name": "Test Conservative Fund",
          "type": "taxable",
          "balance": 200000,
          "return_schedule": "conservative_returns"
        },
        {
          "name": "Test Growth Fund",
          "type": "taxable",
          "balance": 300000,
          "return_schedule": "variable_returns"
        }
      ],
      "rate_schedules": {
        "test_inflation": {
          "type": "fixed",
          "rate": 0.03
        },
        "conservative_returns": {
          "type": "fixed",
          "rate": 0.04
        },
        "variable_returns": {
          "type": "sequence",
          "values": [0.08, 0.06, -0.1, 0.12, 0.05, 0.07],
          "default_rate": 0.06
        }
      }
    },

    "test-flexible-matching": {
      "metadata": {
        "title": "Test: Flexible Content Matching",
        "description": "This scenario is designed to test flexible story-to-scenario matching",
        "tags": ["test", "flexible", "matching", "story-integration"],
        "difficulty": "beginner",
        "author": "System Test"
      },
      "plan": {
        "monthly_expenses": 3500,
        "duration_months": 100
      },
      "assets": [
        {
          "name": "Test Account",
          "type": "taxable",
          "balance": 150000,
          "interest_rate": 0.05
        }
      ]
    }
  }
