{
  "$schema": "../docs/scenario-schema.json",
  
  "baseline-no-conversions": {
    "metadata": {
      "title": "Baseline Strategy - No Conversions",
      "description": "Sequential withdrawals with no tax optimization - higher lifetime taxes",
      "tags": ["baseline", "no-optimization", "comparison"]
    },
    "plan": {
      "monthly_expenses": 7400,
      "duration_months": 300,
      "inflation_schedule": "realistic_inflation",
      "stop_on_shortfall": true
    },
    "income": [
      {
        "name": "Early Social Security",
        "amount": 2500,
        "start_month": 8,
        "notes": "Early claiming with reduced benefits"
      },
      {
        "name": "Full Social Security",
        "amount": 3469,
        "start_month": 84,
        "notes": "Full retirement age at 67"
      }
    ],
    "assets": [
      {
        "name": "High-Yield Savings",
        "type": "taxable",
        "balance": 129000,
        "min_balance": 30000,
        "return_schedule": "savings_rate"
      },
      {
        "name": "Brokerage Investment",
        "type": "taxable",
        "balance": 518000,
        "return_schedule": "market_growth"
      },
      {
        "name": "Traditional IRA",
        "type": "tax_deferred",
        "balance": 320000,
        "return_schedule": "ira_growth",
        "notes": "Subject to RMDs at 73, taxed at withdrawal"
      },
      {
        "name": "Roth IRA",
        "type": "tax_free",
        "balance": 12000,
        "return_schedule": "roth_growth"
      }
    ],
    "order": [
      {"account": "High-Yield Savings", "order": 1},
      {"account": "Brokerage Investment", "order": 2},
      {"account": "Traditional IRA", "order": 3},
      {"account": "Roth IRA", "order": 4}
    ],
    "rate_schedules": {
      "realistic_inflation": {"type": "fixed", "rate": 0.035},
      "savings_rate": {"type": "fixed", "rate": 0.037},
      "market_growth": {"type": "fixed", "rate": 0.065},
      "ira_growth": {"type": "fixed", "rate": 0.062},
      "roth_growth": {"type": "fixed", "rate": 0.07}
    }
  },

